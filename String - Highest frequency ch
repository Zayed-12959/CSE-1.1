#include<stdio.h>

int main(){

    char str[1000];
    int i,j;
    int maxfreq = 0;
    int maxchar;

    printf("Enter a string:");
    fgets(str, sizeof(str), stdin);

    for(i-0; str[i]!='\0'; i++){
        if(str[i]=='\n'){
            continue;
        }
        int count = 0;
        for(j=0; str[j]!='\0'; j++){
            if(str[j]==str[i]){
                count++;
            }
        }
        if(count>maxfreq){
            maxfreq = count;
            maxchar = str[i];
        }
    }

    if(maxfreq>0){
        printf("Highest frequency character is %c with the frequency of %d.\n", maxchar, maxfreq);
    }
    else{
        printf("No characters found");
    }

return 0;
}
