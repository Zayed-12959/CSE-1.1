#include <stdio.h>

int compareStrings(char str1[], char str2[]){
    int i;
    
    // Loop through both strings until a mismatch is found or end of either string
    for(i=0; str1[i]==str2[i]; i++){
        if(str1[i]=='\0'){
            return 0; // Strings are equal
        }
    }

    return (str1[i]-str2[i]); // Return the difference of the first mismatching character
}

int main() {
    char str1[]="hello";
    char str2[]="hello";
    char str3[]="world";

    int result1=compareStrings(str1, str2);
    int result2=compareStrings(str1, str3);

    printf("Comparison 1: %d\n", result1); // Expected Output: 0 (equal strings)
    printf("Comparison 2: %d\n", result2); // Expected Output: Non-zero (not equal)

    return 0;
}
