#include<stdio.h>

int main(){

    int n;
    printf("Enter number of elements:");
    scanf("%d", &n);

    int arr[n];
    int visited[n];

    int i,j;
    for(i=0; i<n; i++){
        printf("Element %d:", i);
        scanf("%d", &arr[i]);
        visited[i]=0;
    }

    //print array
    for(i=0; i<n; i++){
        printf("%d ", arr[i]);
    }
    printf("\n");

    //count
    for(i=0; i<n; i++){
            if(visited[i]==1)
                continue;
            int count=0;
        for(j=0; j<n; j++){
            if(arr[j]==arr[i]){
                count++;
                visited[j]=1;
            }
        }
        printf("%d appears: %d times\n", arr[i], count);
    }

return 0;
}
