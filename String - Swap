#include<stdio.h>
#include<string.h>

int main(){

    char str1[30];
    char str2[30];

    printf("Enter 2 strings:\n");
    gets(str1);
    gets(str2);

    printf("String 1: %s\n", str1);
    printf("String 2: %s\n", str2);

    char temp[30];

    strcpy(temp, str1);
    strcpy(str1, str2);
    strcpy(str2, temp);

    printf("After swapping\n");
    printf("String 1: %s\n", str1);
    printf("String 2: %s\n", str2);

return 0;
}
