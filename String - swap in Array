#include<stdio.h>

int strlen(char string[]){
    int count = 0;
    for(int i=0; string[i]!='\0'; i++){
        count++;
    }
return count;
}

void strcpy(char dest[], char src[]){
    int i;
    for(i=0; i<strlen(src); i++){
        dest[i] = src[i];
    }
    dest[i] = '\0';
}

void strswap(char str1[], char str2[]){
    char temp[100];
    strcpy(temp, str1);
    strcpy(str1, str2);
    strcpy(str2, temp);
}

int main(){

    int n;
    printf("Enter number of strings int the array;");
    scanf("%d", &n);
    getchar();

    char string[n][100];

    printf("Enter the array of string:\n");
    for(int i=0; i<n; i++){
        gets(string[i]);
    }

    int pos1, pos2;
    printf("Enter positions of strings you want to change:\n");
    scanf("%d%d", &pos1, &pos2);

    strswap(string[pos1], string[pos2]);

    printf("Array of strings after swapping:\n");
    for(int i=0; i<n; i++){
        puts(string[i]);
    }

return 0;
}
