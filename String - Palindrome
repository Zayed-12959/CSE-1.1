#include<stdio.h>
#include<string.h>

int main(){

    char str1[30];
    char str2[30];

    printf("Enter first string:");
    gets(str1);

    printf("string 1 : %s\n", str1);

    int i,j;
    int len = 0;

    for(i=0; str1[i]!='\0'; i++){
        len++;
    }
    for(j=0,i=len-1; i>=0; j++,i--){
        str2[j] = str1[i];
    }
    str2[j] = '\0';

    printf("Reversed String: %s\n", str2);

    int d = strcmp(str2,str1);

    if(d==0){
        printf("palindrome");
    }
    else{
        printf("Not palindrome");
    }

return 0;
}

