#include<stdio.h>
#include<ctype.h>

int main(){

    char str[100];
    int alpha = 0;
    int digit = 0;
    int special = 0;

    fgets(str, sizeof(str), stdin);

    for(int i=0; str[i]!='\0'; i++){
        if(isalpha(str[i])){
            alpha++;
        }
        else if(isdigit(str[i])){
            digit++;
        }
        else if(ispunct(str[i])){
            special++;
        }
    }

        printf("alphabets:%d\ndigits:%d\nspecial:%d", alpha, digit, special);

return 0;
}
